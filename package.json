    "clsx": "^2.0.0",
    "eslint": "8.53.0",
    "eslint-config-next": "14.0.1",
    "lucide-react": "^0.292.0",
    "next": "14.0.1",
    "next-themes": "^0.2.1",
    "postcss": "8.4.31",
    "prisma": "^5.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-world-flags": "^1.6.0",
